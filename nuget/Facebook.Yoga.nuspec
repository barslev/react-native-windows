<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Facebook.Yoga</id>
    <version>1.9.0</version>
    <title>Facebook.Yoga</title>
    <authors>Facebook</authors>
    <owners>Facebook</owners>
    <projectUrl>https://github.com/facebook/css-layout</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A subset of CSS's flexbox layout algorithm and box model.</description>
    <copyright>Copyright 2016 Facebook</copyright>
    <tags>flexbox flex-box css layout css-layout yoga facebook native</tags>
  </metadata>
  <files>
    <!-- build -->
    <file src="Facebook.Yoga.targets" target="build\net46" />
    <file src="Facebook.Yoga.dll.config" target="build\net46" />

    <!-- net46 -->
    <file src="..\Yoga\csharp\Windows\Facebook.Yoga\bin\Release\Facebook.Yoga.dll" target="lib\net46" />
    <file src="Facebook.Yoga.dll.config" target="lib\net46" />
  
    <!-- runtimes -->  
    <file src="..\Yoga\csharp\Windows\Facebook.Yoga\bin\x64\Release\Facebook.Yoga.dll" target="runtimes\win-x64\lib\netstandard1.0"/>
    <file src="..\Yoga\csharp\Windows\Facebook.Yoga\bin\x64\Release\Facebook.Yoga.pdb" target="runtimes\win-x64\lib\netstandard1.0"/>

    <!-- Native -->
    <file src="..\Yoga\csharp\Yoga\bin\x64\Release\Yoga.dll" target="runtimes\win-x64\native"/>
    <file src="..\Yoga\csharp\Yoga\bin\x64\Release\Yoga.pdb" target="runtimes\win-x64\native"/> 
  </files>
</package>